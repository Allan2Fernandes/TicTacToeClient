<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TicTacToeClient.Views.HighScoresPage"
             Title="HighScoresPage">
    <VerticalStackLayout>
        <Label x:Name="MyWinsLabel" Margin="150" HorizontalOptions="Center" Text="{Binding MyWinsLabelString}"></Label>
        <Label x:Name="OpponentWinsLabel" Margin="20" HorizontalOptions="Center" Text="{Binding OpponentWinsLabelString}"></Label>
    </VerticalStackLayout>
</ContentPage>